<!DOCTYPE html>
<html>
<head>
  <title>Climbr</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <link href='https://fonts.googleapis.com/css?family=Monoton|Doppio+One' rel='stylesheet' type='text/css'>  
  <script>window.current_user_id = "<%= current_user ? current_user.id : nil %>"</script>
  <script>window.current_user_email = "<%= current_user ? current_user.email : nil %>"</script>
  <script src="//widget.cloudinary.com/global/all.js" type="text/javascript"></script>
  <script type="text/javascript">
    window.CLOUDINARY_OPTIONS = {
      cloud_name: "<%=ENV['CLOUD_NAME']%>",
      upload_preset: "<%=ENV['UPLOAD_PRESET']%>"
    };
  </script>
</head>
<body>

  <script
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDi9poPo6P1iE2mazrRiNPH97oZqXRWiZU">
  </script>

  <%= yield %>

</body>
</html>
